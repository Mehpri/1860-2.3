function AbsoluteValue.compute 0
    push argument 0
    push constant 0
    lt
    if-goto NEGATIVE_CASE
    push constant 0
    pop argument 2
    push constant 0
    pop argument 3
    push argument 0
    pop argument 1
    goto END

label NEGATIVE_CASE
    push constant 1
    pop argument 2
    push argument 0
    push constant 32768
    neg
    eq
    if-goto OVERFLOW_CASE
    push constant 0
    pop argument 3
    push argument 0
    neg
    pop argument 1
    goto END

label OVERFLOW_CASE
    push constant 1
    pop argument 3
    push argument 0
    pop argument 1

label END
    return